{
  "commands": [
    {
      "command": "query",
      "args": {
        "ontology": "HP",
        "query": "hypertension",
        "out": "./out/labels.txt",
        "expand-labels": true
      }
    },
    {
      "command": "get_abstracts",
      "args": {
        "labels": "./out/labels.txt",
        "out": "./out/abstracts/"
      }
    },
    {
      "command": "annotate",
      "args": {
        "text": "./out/abstracts/",
        "labels": "./out/labels.txt",
        "out": "./out/annotations.txt"
      }
    }
  ]
}
