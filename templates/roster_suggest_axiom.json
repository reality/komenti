{
  "commands": [
    {
      "id": "class_query",
      "command": "query",
      "args": {
        "ontology": "HP",
        "class-list": "",
        "out": "./out/labels.txt",
        "override-group": "class"
      }
    },
    {
      "id": "entity_query",
      "command": "query",
      "args": {
        "ontology": "HP",
        "class-list": "",
        "out": "./out/labels.txt",
        "append": true,
        "query-type": "equivalent",
        "override-group": "entity"
      }
    },
    {
      "id": "quality_query",
      "command": "query",
      "args": {
        "ontology": "HP",
        "class-list": "",
        "out": "./out/labels.txt",
        "append": true,
        "expand-labels": true,
        "override-group": "quality"
      }
    },
    {
      "id": "relation_query",
      "command": "query",
      "args": {
        "ontology": "RO",
        "object-properties": "true",
        "out": "./out/labels.txt",
        "append": true,
        "expand-labels": true,
        "override-group": "relation"
      }
    },
    {
      "command": "get_abstracts",
      "args": {
        "labels": "./out/labels.txt",
        "out": "./out/abstracts/",
        "group-by-query": true,
        "exclude-groups": "entity,quality,relation",
        "conjunction": true
      }
    },
    {
      "command": "annotate",
      "args": {
        "text": "./out/abstracts/",
        "labels": "./out/labels.txt",
        "out": "./out/annotations.txt"
      }
    },
    {
      "command": "suggest_axiom",
      "args": {
        "annotation-file": "./out/annotations.txt",
        "labels": "./out/labels.txt"
      }
    }
  ]
}
